{
  "name": "windsurf-tool",
  "version": "6.4.1",
  "description": "Windsurf-Tool - 自动化管理 Windsurf 账号，支持批量注册、账号切换、到期提醒等功能",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron-vite dev",
    "build:vite": "electron-vite build",
    "build": "electron-builder",
    "build:all": "npm run build:mac && npm run build:win && npm run build:linux",
    "build:mac": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac",
    "build:mac:x64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --x64",
    "build:mac:arm64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --arm64",
    "build:mac:interactive": "./build-mac.sh",
    "build:win": "electron-builder --win",
    "build:win:x64": "electron-builder --win --x64",
    "build:win:arm64": "electron-builder --win --arm64",
    "build:win:docker": "./build-docker.sh",
    "build:linux": "electron-builder --linux",
    "build:linux:x64": "electron-builder --linux --x64",
    "test:switch-with-reset": "electron test-account-switch-with-reset.js",
    "postinstall": "echo 'Installation complete'"
  },
  "keywords": [
    "windsurf",
    "account",
    "automation",
    "registration",
    "management"
  ],
  "author": "crispvibe",
  "license": "MIT",
  "homepage": "https://github.com/crispvibe/Windsurf-Tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/crispvibe/Windsurf-Tool.git"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dunder-proto": "^1.0.1",
    "imap": "^0.8.19",
    "lucide": "^0.555.0",
    "mailparser": "^3.6.5",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "asarmor": "^3.0.2",
    "cross-env": "^10.1.0",
    "electron": "^26.6.10",
    "electron-builder": "^24.13.3",
    "electron-vite": "^4.0.1",
    "javascript-obfuscator": "^4.1.1",
    "node-abi": "^4.17.0",
    "vite": "^7.2.4",
    "wine-darwin": "^1.9.17-1"
  },
  "build": {
    "appId": "com.crispvibe.windsurf.tool",
    "productName": "Windsurf-Tool",
    "copyright": "Copyright © 2024 crispvibe",
    "asar": true,
    "asarUnpack": [
      "node_modules/imap/**/*",
      "node_modules/mailparser/**/*",
      "node_modules/utf7/**/*",
      "node_modules/readable-stream/**/*",
      "node_modules/safer-buffer/**/*",
      "node_modules/string_decoder/**/*",
      "node_modules/util-deprecate/**/*",
      "node_modules/inherits/**/*",
      "node_modules/encoding-japanese/**/*",
      "node_modules/he/**/*",
      "node_modules/html-to-text/**/*",
      "node_modules/@selderee/**/*",
      "node_modules/selderee/**/*",
      "node_modules/parseley/**/*",
      "node_modules/leac/**/*",
      "node_modules/peberminta/**/*",
      "node_modules/htmlparser2/**/*",
      "node_modules/dom-serializer/**/*",
      "node_modules/domhandler/**/*",
      "node_modules/domutils/**/*",
      "node_modules/entities/**/*",
      "node_modules/domelementtype/**/*",
      "node_modules/iconv-lite/**/*",
      "node_modules/libmime/**/*",
      "node_modules/libbase64/**/*",
      "node_modules/libqp/**/*",
      "node_modules/linkify-it/**/*",
      "node_modules/uc.micro/**/*",
      "node_modules/nodemailer/**/*",
      "node_modules/punycode.js/**/*",
      "node_modules/tlds/**/*",
      "node_modules/@zone-eu/**/*",
      "node_modules/sql.js/**/*",
      "node_modules/uuid/**/*",
      "node_modules/axios/**/*",
      "node_modules/follow-redirects/**/*",
      "node_modules/form-data/**/*",
      "node_modules/es-set-tostringtag/**/*",
      "node_modules/hasown/**/*",
      "node_modules/es-errors/**/*",
      "node_modules/get-intrinsic/**/*",
      "node_modules/has-tostringtag/**/*",
      "node_modules/function-bind/**/*",
      "node_modules/call-bind-apply-helpers/**/*",
      "node_modules/es-define-property/**/*",
      "node_modules/es-object-atoms/**/*",
      "node_modules/get-proto/**/*",
      "node_modules/dunder-proto/**/*",
      "node_modules/gopd/**/*",
      "node_modules/has-symbols/**/*",
      "node_modules/math-intrinsics/**/*",
      "node_modules/proxy-from-env/**/*",
      "node_modules/combined-stream/**/*",
      "node_modules/delayed-stream/**/*",
      "node_modules/mime-types/**/*",
      "node_modules/mime-db/**/*",
      "node_modules/asynckit/**/*"
    ],
    "compression": "normal",
    "afterPack": "./build-scripts/afterPack.js",
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "main.js",
      "index.html",
      "language-selector.html",
      "renderer.js",
      "js/**/*",
      "src/**/*",
      "bin/**/*",
      "pay/**/*",
      "node_modules/lucide/**/*",
      "node_modules/imap/**/*",
      "node_modules/mailparser/**/*",
      "node_modules/utf7/**/*",
      "node_modules/readable-stream/**/*",
      "node_modules/safer-buffer/**/*",
      "node_modules/string_decoder/**/*",
      "node_modules/util-deprecate/**/*",
      "node_modules/inherits/**/*",
      "node_modules/encoding-japanese/**/*",
      "node_modules/he/**/*",
      "node_modules/html-to-text/**/*",
      "node_modules/@selderee/**/*",
      "node_modules/selderee/**/*",
      "node_modules/parseley/**/*",
      "node_modules/leac/**/*",
      "node_modules/peberminta/**/*",
      "node_modules/htmlparser2/**/*",
      "node_modules/dom-serializer/**/*",
      "node_modules/domhandler/**/*",
      "node_modules/domutils/**/*",
      "node_modules/entities/**/*",
      "node_modules/domelementtype/**/*",
      "node_modules/iconv-lite/**/*",
      "node_modules/libmime/**/*",
      "node_modules/libbase64/**/*",
      "node_modules/libqp/**/*",
      "node_modules/linkify-it/**/*",
      "node_modules/uc.micro/**/*",
      "node_modules/nodemailer/**/*",
      "node_modules/punycode.js/**/*",
      "node_modules/tlds/**/*",
      "node_modules/@zone-eu/**/*",
      "node_modules/sql.js/**/*",
      "node_modules/uuid/**/*",
      "node_modules/axios/**/*",
      "node_modules/follow-redirects/**/*",
      "node_modules/form-data/**/*",
      "node_modules/es-set-tostringtag/**/*",
      "node_modules/hasown/**/*",
      "node_modules/es-errors/**/*",
      "node_modules/get-intrinsic/**/*",
      "node_modules/has-tostringtag/**/*",
      "node_modules/function-bind/**/*",
      "node_modules/call-bind-apply-helpers/**/*",
      "node_modules/es-define-property/**/*",
      "node_modules/es-object-atoms/**/*",
      "node_modules/get-proto/**/*",
      "node_modules/dunder-proto/**/*",
      "node_modules/gopd/**/*",
      "node_modules/has-symbols/**/*",
      "node_modules/math-intrinsics/**/*",
      "node_modules/proxy-from-env/**/*",
      "node_modules/combined-stream/**/*",
      "node_modules/delayed-stream/**/*",
      "node_modules/mime-types/**/*",
      "node_modules/mime-db/**/*",
      "node_modules/asynckit/**/*",
      "package.json",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "timestamp": false,
      "identity": null,
      "icon": "build/icon.png",
      "target": [
        "dmg"
      ],
      "artifactName": "${productName}-${version}-macOS-${arch}.${ext}",
      "entitlements": null,
      "entitlementsInherit": null,
      "type": "distribution",
      "minimumSystemVersion": "10.13.0",
      "darkModeSupport": true,
      "mergeASARs": false
    },
    "dmg": {
      "title": "${productName} ${version}",
      "icon": "build/icon.png",
      "background": null,
      "window": {
        "width": 600,
        "height": 450
      },
      "contents": [
        {
          "x": 150,
          "y": 250,
          "type": "file"
        },
        {
          "x": 450,
          "y": 250,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "format": "ULFO",
      "internetEnabled": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "artifactName": "${productName}-Setup-${version}-${arch}.${ext}",
      "requestedExecutionLevel": "asInvoker",
      "verifyUpdateCodeSignature": false,
      "publisherName": "Windsurf Tool",
      "signingHashAlgorithms": [
        "sha256"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Windsurf-Tool",
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "license": "LICENSE",
      "artifactName": "${productName}-Setup-${version}-${arch}.${ext}",
      "displayLanguageSelector": false,
      "installerLanguages": [
        "zh_CN",
        "en_US"
      ],
      "language": "2052",
      "include": "build-scripts/installer.nsh"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "icon": "build/icon.png",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "desktop": {
        "Name": "Windsurf-Tool",
        "Comment": "Windsurf账号管理工具",
        "GenericName": "Account Manager",
        "Categories": "Development;Utility;"
      }
    }
  }
}
